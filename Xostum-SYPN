	local Section22 = Tab2:AddSection("Trolling", {default = false})


	local Box = Section22:AddBox("Kill Player", {fireonempty = false}, function(text)
		for _, player in ipairs(game.Players:GetPlayers()) do
			if player ~= game:GetService("Players").LocalPlayer then
				if player.Name:lower():find(text:lower()) == 1 then
					local args = {[1] = 1,[2] = player}
					game:GetService("ReplicatedStorage").RemoteEvents.ToxicDrown:FireServer(unpack(args))
					if usesounds == true then
						local SDC = SD:Clone()
						SDC.SoundId = "http://www.roblox.com/asset/?id=130819307"
						SDC.Parent = game:GetService("Workspace");SDC:Play()
						wait(1.2)
						SDC:Destroy()
					end
					return
				elseif player.DisplayName:lower():find(text:lower()) == 1 then
					local args = {[1] = 1,[2] = player}
					game:GetService("ReplicatedStorage").RemoteEvents.ToxicDrown:FireServer(unpack(args))
					if usesounds == true then
						local SDC = SD:Clone()
						SDC.SoundId = "http://www.roblox.com/asset/?id=130819307"
						SDC.Parent = game:GetService("Workspace");SDC:Play()
						wait(1.2)
						SDC:Destroy()
					end
					return
				end
			end
		end
	end)

