	local Section25 = Tab5:AddSection("Players", {default = false})
	local Box3 = Section25:AddBox("Teleport To", {fireonempty = false}, function(text)
		for _, player in ipairs(game.Players:GetPlayers()) do
			if player ~= game:GetService("Players").LocalPlayer then
				if player.Name:lower():find(text:lower()) == 1 then
					if usetween == true then
						game:GetService("TweenService"):Create(game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart"),TweenInfo.new(gotospeed,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{CFrame = player.Character:FindFirstChild("HumanoidRootPart").CFrame}):Play()
					elseif IsWalkTo == true then
						repeat wait() until game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid");
						game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):MoveTo(player.Character:FindFirstChild("HumanoidRootPart").Position)
					elseif IsWalkTo == false and usetween == false then
						game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = player.Character:FindFirstChild("HumanoidRootPart").CFrame
						if usesounds == true then
							local SDC = SD:Clone()
							SDC.SoundId = "http://www.roblox.com/asset/?id=5066021887"
							SDC.Parent = game:GetService("Workspace");SDC:Play()
							wait(1.2)
							SDC:Destroy()
						end
						return
					end
					return
				elseif player.DisplayName:lower():find(text:lower()) == 1 then
					if usetween == true then
						game:GetService("TweenService"):Create(game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart"),TweenInfo.new(gotospeed,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{CFrame = player.Character:FindFirstChild("HumanoidRootPart").CFrame}):Play()
					elseif IsWalkTo == true then
						repeat wait() until game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid");
						game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):MoveTo(player.Character:FindFirstChild("HumanoidRootPart").Position)
					elseif IsWalkTo == false and usetween == false then
						game:GetService("Players").LocalPlayer.Character:FindFirstChild("HumanoidRootPart").CFrame = player.Character:FindFirstChild("HumanoidRootPart").CFrame
						if usesounds == true then
							local SDC = SD:Clone()
							SDC.SoundId = "http://www.roblox.com/asset/?id=5066021887"
							SDC.Parent = game:GetService("Workspace");SDC:Play()
							wait(1.2)
							SDC:Destroy()
						end
						return
					end
					return
				end
			end
		end
	end)
	local Slider457686543 = Section25:AddSlider("Tween Speed", 1, 5, 1, {toggleable = true, default = true, flag = "Slider_Flag", fireontoggle = true, fireondrag = true, rounded = true}, function(val, bool)
